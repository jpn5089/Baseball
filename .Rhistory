select(1,11)
colnames(whip) <- c("Name", "WHIP")
c <- merge(a, b, by = "Name") %>%
mutate(pPerIn = Pitches/IP)
final <- merge(c, whip, by = "Name") %>%
filter(IP >= 50) %>%
mutate(SHORTNAME = paste0(Name, " - ", IP, " IP"))
ggplot(final, aes(pPerIn , ERA, label = SHORTNAME)) +
